<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"

    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/accountFragment"
        android:name="com.ketarketir.tiketkuioflight.view.AccountFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_accountFragment_to_loginFragment2"
            app:destination="@id/loginFragment2" />
        <action
            android:id="@+id/action_accountFragment_to_notificationFragment"
            app:destination="@id/notificationFragment" />
        <action
            android:id="@+id/action_accountFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.ketarketir.tiketkuioflight.view.NotificationFragment"
        android:label="fragment_notifikasi"
        tools:layout="@layout/fragment_notification" >
        <action
            android:id="@+id/action_notificationFragment_to_accountFragment"
            app:destination="@id/accountFragment" />
        <action
            android:id="@+id/action_notificationFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.ketarketir.tiketkuioflight.view.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >

        <action
            android:id="@+id/action_registerFragment_to_sendOTPFragment"
            app:destination="@id/sendOTPFragment" />
        <action
            android:id="@+id/action_registerFragment_to_loginFragment22"
            app:destination="@id/loginFragment2" />

    </fragment>
    <fragment
        android:id="@+id/resetPasswordFragment"
        android:name="com.ketarketir.tiketkuioflight.view.ResetPasswordFragment"
        android:label="fragment_reset_password"
        tools:layout="@layout/fragment_reset_password" >
        <action
            android:id="@+id/action_resetPasswordFragment_to_loginFragment2"
            app:destination="@id/loginFragment2" />
    </fragment>
    <fragment
        android:id="@+id/bookingHistoryNonLoginFragment"
        android:name="com.ketarketir.tiketkuioflight.view.BookingHistoryNonLoginFragment"
        android:label="fragment_riwayat_pesanan_non_login"
        tools:layout="@layout/fragment_booking_history_non_login" >
        <action
            android:id="@+id/action_bookingHistoryNonLoginFragment_to_loginFragment2"
            app:destination="@id/loginFragment2" />
        <action
            android:id="@+id/action_bookingHistoryNonLoginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/sendOTPFragment"
        android:name="com.ketarketir.tiketkuioflight.view.SendOTPFragment"
        android:label="fragment_send_o_t_p"
        tools:layout="@layout/fragment_send_o_t_p" >
        <action
            android:id="@+id/action_sendOTPFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.ketarketir.tiketkuioflight.view.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_accountFragment"
            app:destination="@id/accountFragment" />
        <action
            android:id="@+id/action_homeFragment_to_notificationFragment"
            app:destination="@id/notificationFragment" />
        <action
            android:id="@+id/action_homeFragment_to_bookingHistoryNonLoginFragment"
            app:destination="@id/bookingHistoryNonLoginFragment" />
        <action
            android:id="@+id/action_homeFragment_self"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_homeFragment_to_loginFragment2"
            app:destination="@id/loginFragment2" />
        <action
            android:id="@+id/action_homeFragment_to_bookingHistoryLoginFragment"
            app:destination="@id/bookingHistoryLoginFragment" />
    </fragment>

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.ketarketir.tiketkuioflight.view.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_splashFragment_to_loginFragment2"
            app:destination="@id/loginFragment2" />
    </fragment>
    <fragment
        android:id="@+id/bookingHistoryLoginFragment"
        android:name="com.ketarketir.tiketkuioflight.view.BookingHistoryLoginFragment"
        android:label="fragment_booking_history_login"
        tools:layout="@layout/fragment_booking_history_login" />
    <fragment
        android:id="@+id/loginFragment2"
        android:name="com.ketarketir.tiketkuioflight.view.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment2_to_sendOTPFragment"
            app:destination="@id/sendOTPFragment" />
        <action
            android:id="@+id/action_loginFragment2_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment2_to_resetPasswordFragment"
            app:destination="@id/resetPasswordFragment" />
    </fragment>
</navigation>