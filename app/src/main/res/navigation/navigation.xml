<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"


    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.ketarketir.tiketkuioflight.view.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_splashFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.ketarketir.tiketkuioflight.view.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_accountFragment"
            app:destination="@id/accountFragment" />
        <action
            android:id="@+id/action_homeFragment_to_notificationFragment"
            app:destination="@id/notificationFragment" />
        <action
            android:id="@+id/action_homeFragment_to_bottomSheetClassSeatFragment"
            app:destination="@id/bottomSheetClassSeatFragment" />
        <action
            android:id="@+id/action_homeFragment_to_bottomSheetHomeSearchDestinationFragment"
            app:destination="@id/bottomSheetHomeSearchDestinationFragment" />
        <action
            android:id="@+id/action_homeFragment_to_bottomSheetSetPassengerFragment"
            app:destination="@id/bottomSheetSetPassengerFragment" />
        <action
            android:id="@+id/action_homeFragment_to_bottomSheetSearchResultFilterFragment"
            app:destination="@id/bottomSheetSearchResultFilterFragment" />
        <action
            android:id="@+id/action_homeFragment_to_detailDestinationFragment"
            app:destination="@id/detailDestinationFragment" />
        <action
            android:id="@+id/action_homeFragment_to_resultSearchFragment"
            app:destination="@id/resultSearchFragment" />
        <action
            android:id="@+id/action_homeFragment_to_historyFragment"
            app:destination="@id/historyFragment" />
    </fragment>
    <fragment
        android:id="@+id/accountFragment"
        android:name="com.ketarketir.tiketkuioflight.view.AccountFragment"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" >
        <action
            android:id="@+id/action_accountFragment_to_accountSettingFragment"
            app:destination="@id/accountSettingFragment" />
        <action
            android:id="@+id/action_accountFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_accountFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_accountFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.ketarketir.tiketkuioflight.view.NotificationFragment"
        android:label="fragment_notification"
        tools:layout="@layout/fragment_notification" >
        <action
            android:id="@+id/action_notificationFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_notificationFragment_to_accountFragment"
            app:destination="@id/accountFragment" />
    </fragment>
    <fragment
        android:id="@+id/accountSettingFragment"
        android:name="com.ketarketir.tiketkuioflight.view.AccountSettingFragment"
        android:label="fragment_account_setting"
        tools:layout="@layout/fragment_account_setting" >
        <action
            android:id="@+id/action_accountSettingFragment_to_accountFragment"
            app:destination="@id/accountFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.ketarketir.tiketkuioflight.view.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_accountFragment"
            app:destination="@id/accountFragment" />
        <action
            android:id="@+id/action_profileFragment_to_bottomSheetDialogUpdateAccountFragment"
            app:destination="@id/bottomSheetDialogUpdateAccountFragment" />
        <action
            android:id="@+id/action_profileFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/bottomSheetDialogUpdateAccountFragment"
        android:name="com.ketarketir.tiketkuioflight.view.BottomSheetDialogUpdateAccountFragment"
        android:label="fragment_bottom_sheet_dialog_update_account"
        tools:layout="@layout/fragment_bottom_sheet_dialog_update_account" />
    <fragment
        android:id="@+id/forgetPasswordFragment"
        android:name="com.ketarketir.tiketkuioflight.view.ForgetPasswordFragment"
        android:label="ForgetPasswordFragment" />
    <fragment
        android:id="@+id/biodataBookingFragment"
        android:name="com.ketarketir.tiketkuioflight.view.BiodataBookingFragment"
        android:label="fragment_biodata_booking"
        tools:layout="@layout/fragment_biodata_booking" />
    <fragment
        android:id="@+id/sendOTPFragment"
        android:name="com.ketarketir.tiketkuioflight.view.SendOTPFragment"
        android:label="fragment_send_o_t_p"
        tools:layout="@layout/fragment_send_o_t_p" >
        <action
            android:id="@+id/action_sendOTPFragment_to_inputEmailFragment"
            app:destination="@id/inputEmailFragment" />
        <action
            android:id="@+id/action_sendOTPFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.ketarketir.tiketkuioflight.view.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment_to_forgetPasswordFragment"
            app:destination="@id/forgetPasswordFragment" />
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_loginFragment_to_inputEmailFragment"
            app:destination="@id/inputEmailFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.ketarketir.tiketkuioflight.view.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_registerFragment_to_sendOTPFragment"
            app:destination="@id/sendOTPFragment" />
    </fragment>
    <dialog
        android:id="@+id/bottomSheetClassSeatFragment"
        android:name="com.ketarketir.tiketkuioflight.view.BottomSheetClassSeatFragment"
        android:label="fragment_bottom_sheet_class_seat"
        tools:layout="@layout/fragment_bottom_sheet_class_seat" >
        <action
            android:id="@+id/action_bottomSheetClassSeatFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </dialog>
    <fragment
        android:id="@+id/bottomSheetHomeSearchDestinationFragment"
        android:name="com.ketarketir.tiketkuioflight.view.BottomSheetHomeSearchDestinationFragment"
        android:label="fragment_bottom_sheet_home_search_destination"
        tools:layout="@layout/fragment_bottom_sheet_home_search_destination" >
        <action
            android:id="@+id/action_bottomSheetHomeSearchDestinationFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <dialog
        android:id="@+id/bottomSheetSetPassengerFragment"
        android:name="com.ketarketir.tiketkuioflight.view.BottomSheetSetPassengerFragment"
        android:label="fragment_bottom_sheet_set_passenger"
        tools:layout="@layout/fragment_bottom_sheet_set_passenger" >
        <action
            android:id="@+id/action_bottomSheetSetPassengerFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </dialog>
    <fragment
        android:id="@+id/bottomSheetSearchResultFilterFragment"
        android:name="com.ketarketir.tiketkuioflight.view.BottomSheetSearchResultFilterFragment"
        android:label="fragment_bottom_sheet_search_result_filter"
        tools:layout="@layout/fragment_bottom_sheet_search_result_filter" >
        <action
            android:id="@+id/action_bottomSheetSearchResultFilterFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailDestinationFragment"
        android:name="com.ketarketir.tiketkuioflight.view.DetailDestinationFragment"
        android:label="fragment_detail_destination"
        tools:layout="@layout/fragment_detail_destination" >
        <action
            android:id="@+id/action_detailDestinationFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultSearchFragment"
        android:name="com.ketarketir.tiketkuioflight.view.ResultSearchFragment"
        android:label="fragment_result_search"
        tools:layout="@layout/fragment_result_search" >
        <action
            android:id="@+id/action_resultSearchFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/inputEmailFragment"
        android:name="com.ketarketir.tiketkuioflight.view.InputEmailFragment"
        android:label="fragment_input_email"
        tools:layout="@layout/fragment_input_email" >
        <action
            android:id="@+id/action_inputEmailFragment_to_sendOTPFragment"
            app:destination="@id/sendOTPFragment" />
        <action
            android:id="@+id/action_inputEmailFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/newPasswordFragment"
        android:name="com.ketarketir.tiketkuioflight.view.NewPasswordFragment"
        android:label="fragment_new_password"
        tools:layout="@layout/fragment_new_password" >
        <action
            android:id="@+id/action_newPasswordFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.ketarketir.tiketkuioflight.view.HistoryFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_history" >
        <action
            android:id="@+id/action_historyFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>